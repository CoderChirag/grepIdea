{
  "builds": [{ "src": "dist/index.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "(.*)",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      },

      "dest": "dist/index.js",
      "continue": true
    }
  ]
}
